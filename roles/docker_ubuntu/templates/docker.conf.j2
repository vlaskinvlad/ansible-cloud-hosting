[Service]
ExecStart=/usr/bin/docker daemon {% for group in group_names %} --label "g_"{{ group }}"=1" {% endfor %} --dns 8.8.8.8 --dns-search service.consul --userland-proxy=false -H tcp://0.0.0.0:2376 --cluster-store=consul://{{ ansible_eth0.ipv4.address }}:8500  --tlsverify --tlscacert={{ docker_tls_ca_cert_file }} --tlscert={{ docker_tls_server_cert_file }} --tlskey={{ docker_tls_server_key_file }}
